		../images/searchfind
	$(UIC)  -embed afedit ../images/filenew ../images/fileopen ../images/filesave ../images/print ../images/undo ../images/redo ../images/editcut ../images/editcopy ../images/editpaste ../images/searchfind -o .ui/qmake_image_collection.cpp

